mnply:Terrain
      a owl:Class, rdfs:Class ;
      rdfs:comment "Un Terrain" ;
      rdfs:isDefinedBy mnply: ;
      rdfs:label "Terrain" ;
      vs:term_status "stable" .
      
mnply:Speciale
	a owl:Class, rdfs:Class ;
	rdfs:comment "Une case speciale" ;
	rdfs:isDefinedBy mnply: ;
	rdfs:label "Speciale" ;
	vs:term_status "stable" .

mnply:Case
	a owl:Class, rdfs:Class ;
